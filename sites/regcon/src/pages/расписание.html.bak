                    <!-- Форма заявки -->
<div id="form-modal" class="modal" style="display: none;">
    <div class="modal-content">
        <span id="close-modal" class="close">&times;</span>
        <h2>Подать заявку</h2>
        <form id="application-form">
            <!-- Название программы -->
            <label for="program-name">Название программы</label>
            <input type="text" id="program-name" name="program-name" readonly>

            <!-- Контактное лицо -->
            <label for="contact-type">Контактное лицо</label>
            <select id="contact-type" name="contact-type" required>
                <option value="individual">Физическое лицо</option>
                <option value="organization">Юридическое лицо</option>
            </select>

            <!-- Поля для физического лица -->
            <div id="individual-fields" style="display: none;">
                <label for="individual-name">ФИО</label>
                <input type="text" id="individual-name" name="individual-name">
            </div>

            <!-- Поля для юридического лица -->
            <div id="organization-fields" style="display: none;">
                <label for="organization-name">Название организации</label>
                <input type="text" id="organization-name" name="organization-name">

                <label for="organization-card">Прикрепите карточку организации</label>
                <div class="file-upload">
                    <input type="file" id="organization-card" name="organization-card" accept="image/*">
                    <span id="file-name">Файл не выбран</span>
                    <button type="button" id="remove-file" style="display: none;">&times;</button>
                </div>
            </div>

            <!-- Формат обучения -->
            <label for="training-format">Формат обучения</label>
            <select id="training-format" name="training-format" required>
                <option value="open">Открытое обучение</option>
                <option value="corporate">Корпоративное обучение</option>
                <option value="distance">Дистанционное обучение</option>
            </select>

            <form id="application-form">
                <!-- Поле выбора дат -->
                <label for="training-dates">Даты проведения</label>
                <input type="text" id="training-dates" name="training-dates" placeholder="Выберите даты" readonly>
            </form>
            
            <script>
                // Инициализация flatpickr
                document.addEventListener('DOMContentLoaded', () => {
                    flatpickr("#training-dates", {
                        mode: "multiple", // Режим выбора нескольких дат
                        dateFormat: "d.m.Y", // Формат отображения даты
                        locale: "ru", // Локализация календаря
                        altInput: true, // Альтернативное отображение
                        altFormat: "d.m.Y", // Формат для альтернативного отображения
                        onChange: (selectedDates, dateStr) => {
                            console.log("Выбранные даты:", dateStr); // Логируем выбранные даты
                        }
                    });
                });
            </script>

            <!-- Телефон -->
            <label for="contact-number">Телефон</label>
            <input type="tel" id="contact-number" name="contact-number" placeholder="+7 (999) 999-99-99" required maxlength="18">

            <!-- Email -->
            <label for="email">E-mail</label>
            <input type="email" id="email" name="email" placeholder="example@gmail.com" required>

            <!-- Кнопка отправки -->
            <button type="submit">Отправить</button>
        </form>
    </div>
