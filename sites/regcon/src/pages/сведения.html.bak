        </div>
        <script>

document.querySelectorAll('.menu-header').forEach(button => {
    button.addEventListener('click', (event) => {
        const content = button.nextElementSibling; // Контент, связанный с текущей кнопкой
        const arrow = button.querySelector('.arrow');
        const isNested = button.closest('.menu-content'); // Проверка, находится ли кнопка внутри другого меню

        // Останавливаем всплытие событий, чтобы вложенные кнопки не срабатывали на родительском меню
        event.stopPropagation();

        // Закрытие других меню, если это не вложенное
        if (!isNested) {
            const activeButtons = document.querySelectorAll('.menu-header.active');
            activeButtons.forEach(activeButton => {
                if (activeButton !== button) {
                    activeButton.classList.remove('active');
                    activeButton.nextElementSibling.classList.remove('open');
                    activeButton.querySelector('.arrow').style.transform = 'rotate(0deg)';
                }
            });
        }

        // Открытие/закрытие текущего меню
        button.classList.toggle('active');
        content.classList.toggle('open');  // Добавляем класс .open для плавного раскрытия
        arrow.style.transform = content.classList.contains('open') ? 'rotate(90deg)' : 'rotate(0deg)';
    });
});





        </script>


