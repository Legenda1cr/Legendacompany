      <script>
                    const cachelTrack = document.querySelector('.cachel-track');
      const cachelPrev = document.querySelector('.cachel-prev');
      const cachelNext = document.querySelector('.cachel-next');
      let currentSlideIndex = 0;
      const slideGroups = document.querySelectorAll('.cachel-slide-group');
      const totalGroups = slideGroups.length;

      function showNextGroup() {
          currentSlideIndex = (currentSlideIndex + 1) % totalGroups;
          const groupWidth = slideGroups[0].offsetWidth;
          cachelTrack.style.transform = `translateX(-${currentSlideIndex * groupWidth}px)`;
      }

      function showPrevGroup() {
          currentSlideIndex = (currentSlideIndex - 1 + totalGroups) % totalGroups;
          const groupWidth = slideGroups[0].offsetWidth;
          cachelTrack.style.transform = `translateX(-${currentSlideIndex * groupWidth}px)`;
      }

      cachelNext.addEventListener('click', showNextGroup);
      cachelPrev.addEventListener('click', showPrevGroup);


      </script>
  
