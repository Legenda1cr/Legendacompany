                        <script>
                          document.addEventListener("DOMContentLoaded", () => {
                      const cachelTrack = document.querySelector(".cachel-track");
                      const cachelSlides = document.querySelectorAll(".cachel-slide");
                      const cachelPrev = document.querySelector(".cachel-prev");
                      const cachelNext = document.querySelector(".cachel-next");

                      let currentIndex = 0; // Индекс текущего слайда
                      let visibleSlides = getVisibleSlides(); // Количество видимых слайдов
                      const totalSlides = cachelSlides.length;

                      // Вычисляем ширину одного слайда
                      function getSlideWidth() {
                          return cachelSlides[0].offsetWidth; // Используем offsetWidth для более точного измерения
                      }

                      // Определяем количество видимых слайдов в зависимости от ширины экрана
                      function getVisibleSlides() {
                          return window.innerWidth < 700 ? 1 : 3; // 1 слайд для мобильных, 3 — для десктопа
                      }

                      // Обновляем позицию слайдов
                      function updateSliderPosition() {
                          const slideWidth = getSlideWidth(); // Ширина одного слайда
                          const offset = currentIndex * slideWidth; // Вычисляем точное смещение
                          cachelTrack.style.transform = `translateX(-${offset}px)`;
                          cachelTrack.style.transition = "transform 0.5s ease-in-out";
                      }

                      // Переход к следующему слайду
                      function showNextSlides() {
                          if (currentIndex < totalSlides - visibleSlides) {
                              currentIndex++;
                              updateSliderPosition();
                          }
                      }

                      // Переход к предыдущему слайду
                      function showPrevSlides() {
                          if (currentIndex > 0) {
                              currentIndex--;
                              updateSliderPosition();
                          }
                      }

                      // Обновление видимых слайдов при изменении размера окна
                      window.addEventListener("resize", () => {
                          visibleSlides = getVisibleSlides(); // Обновляем количество видимых слайдов
                          currentIndex = Math.min(currentIndex, totalSlides - visibleSlides); // Ограничиваем индекс
                          updateSliderPosition(); // Обновляем положение трека
                      });

                      // Обработчики кнопок
                      cachelNext.addEventListener("click", showNextSlides);
                      cachelPrev.addEventListener("click", showPrevSlides);

                      // Инициализация
                      updateSliderPosition();
                  });


                        </script>
